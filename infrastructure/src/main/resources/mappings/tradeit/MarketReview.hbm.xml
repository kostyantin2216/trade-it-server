<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.charlieaffs.data.tradeit.model.MarketReview" table="MarketReview" catalog="services" 
		   dynamic-insert="true">
		<id name="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="title" type="java.lang.String" />
		<property name="content" type="java.lang.String" />
		<property name="imageUrl" type="java.lang.String" />
		<property name="updatedAt" type="java.util.Date" />
		<property name="createdAt" type="java.util.Date" />
	</class>
</hibernate-mapping>