<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.charlieaffs.data.tradeit.model.Broker" table="Broker" catalog="services" 
		   dynamic-insert="true">
		<id name="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="name" type="java.lang.String" />
		<property name="description" type="java.lang.String" />
		<property name="logoUrl" type="java.lang.String" />
		<property name="minDeposit" type="java.lang.Integer" />
		<property name="minWithdrawal" type="java.lang.Integer" />
		<property name="promotion" type="java.lang.String" />
		<property name="rating" type="java.lang.Float" />
		<property name="createdAt" type="java.util.Date" />
		<property name="updatedAt" type="java.util.Date" />
		<property name="url" type="java.lang.String" />
		<property name="isRecommended" type="java.lang.Boolean" />
		<property name="isActive" type="java.lang.Boolean" />
	</class>
</hibernate-mapping>