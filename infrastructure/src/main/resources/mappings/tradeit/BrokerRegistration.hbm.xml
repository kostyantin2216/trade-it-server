<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.charlieaffs.data.tradeit.model.BrokerRegistration" table="BrokerRegistration" catalog="services" 
		   dynamic-insert="true">
		<id name="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="userId" type="java.lang.Integer" />
		<property name="brokerId" type="java.lang.Integer" /> 
		<property name="userBrokerId" type="java.lang.String" />
		<property name="message" type="java.lang.String" />
		<property name="isSuccess" type="java.lang.Boolean" />
		<property name="trackingId" type="java.lang.String" />
		<property name="ipAddress" type="java.lang.String" />
		<property name="createdAt" type="java.util.Date" />
	</class>
</hibernate-mapping>