<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.charlieaffs.data.tradeit.model.ContentPage" table="ContentPage" catalog="services" 
		   dynamic-insert="true">
		<id name="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="pageTypeId" type="java.lang.Integer" />
		<property name="title" type="java.lang.String" />
		<property name="description" type="java.lang.String" />
		<property name="androidIconResId" type="java.lang.String" />
		<property name="templateFileName" type="java.lang.String" />
		<property name="htmlContent" type="java.lang.String" />
		<property name="createdAt" type="java.util.Date" />
		<property name="updatedAt" type="java.sql.Timestamp" />
	</class>
</hibernate-mapping>