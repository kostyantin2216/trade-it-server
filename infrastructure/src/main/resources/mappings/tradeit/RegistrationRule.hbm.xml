<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.charlieaffs.data.tradeit.model.RegistrationRule" table="RegistrationRule" catalog="services" 
		   dynamic-insert="true">
		<id name="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="brokerId" type="java.lang.Integer" />
		<property name="countryId" type="java.lang.Integer" />
		<property name="isActive" type="java.lang.Boolean" />
		<property name="updatedAt" type="java.util.Date" />
	</class>
</hibernate-mapping>